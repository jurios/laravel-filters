{
  "name": "kodilab/laravel-filters",
  "description": "Laravel filtering system",
  "keywords": ["laravel", "query", "filters"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jordi",
      "email": "jordi@kodilab.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.5.*|5.6.*|5.7.*",
    "illuminate/database": "5.5.*|5.6.*|5.7.*",
    "illuminate/http": "5.5.*|5.6.*|5.7.*",
    "illuminate/pagination": "5.5.*|5.6.*|5.7.*",
    "illuminate/contracts": "5.5.*|5.6.*|5.7.*"
  },
  "require-dev": {
    "orchestra/testbench": "~3.0",
    "fzaninotto/faker": "^1.4",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Kodilab\\LaravelFilters\\": "src/",
      "Kodilab\\LaravelFilters\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kodilab\\LaravelFilters\\FiltersProvider"
      ]
    }
  }
}
